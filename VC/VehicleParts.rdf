	</owl:Axiom>
	<owl:Axiom>
		<rdfs:seeAlso>https://www.law.cornell.edu/cfr/text/49/571.214</rdfs:seeAlso>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ModifiedRoof"/>
