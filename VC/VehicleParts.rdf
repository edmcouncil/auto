		<skos:definition xml:lang="en">system in a vehicle that changes gears at different speeds</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;Tread">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Tire"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">tread</rdfs:label>
		<skos:definition xml:lang="en">portion of a tire that comes into contact with the road</skos:definition>
