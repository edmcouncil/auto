		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Door"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">window shade</rdfs:label>
		<rdfs:comment xml:lang="en">Signal/Attribute.Cabin.Door.RowX.Left/Right.Shade : Side window shade</rdfs:comment>
