		<fibo-fnd-utl-av:adaptedFrom>49 C.F.R. § 571.206</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<rdfs:seeAlso>https://www.law.cornell.edu/cfr/text/49/571.206</rdfs:seeAlso>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;DoubleDoor"/>
