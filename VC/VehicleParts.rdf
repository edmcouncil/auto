		<fibo-fnd-utl-av:adaptedFrom>49 C.F.R. § 571.217</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<rdfs:seeAlso>https://www.law.cornell.edu/cfr/text/49/571.223</rdfs:seeAlso>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;HydraulicGuard"/>
