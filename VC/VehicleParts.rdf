		<skos:definition xml:lang="en">interior space of a vehicle</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;CarBed">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
