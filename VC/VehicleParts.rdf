	</owl:Axiom>
	<owl:Axiom>
		<rdfs:seeAlso>https://www.law.cornell.edu/cfr/text/49/571.217</rdfs:seeAlso>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
