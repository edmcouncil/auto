	<owl:Class rdf:about="&auto-vp;VehiclePart">
		<rdfs:subClassOf rdf:resource="&auto-mo;MaterialEntity"/>
		<rdfs:label xml:lang="en">vehicle part</rdfs:label>
		<skos:definition xml:lang="en">article the primary use of which is to improve, repair, replace, or serve as a component part of a vehicle</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;VehicleSystem">
		<rdfs:subClassOf rdf:resource="&auto-mo;MaterialEntity"/>
