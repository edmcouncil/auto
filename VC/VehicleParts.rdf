		<skos:definition xml:lang="en">polished or smooth surfaces (as of glass) that form images by reflection</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;ModifiedRoof">
		<rdfs:subClassOf rdf:resource="&auto-vp;Roof"/>
		<rdfs:label>modified roof</rdfs:label>
