		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">cord</rdfs:label>
		<skos:definition xml:lang="en">set of strands forming the plies in the tire</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;CruiseControl">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
