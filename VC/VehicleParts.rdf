				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Cabin"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">bullet resistant shield</rdfs:label>
		<skos:definition xml:lang="en">shield or barrier that is installed completely inside a motor vehicle behind and separate from glazing materials that prevents bullets from coming through it</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;Cabin">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
		<rdfs:label xml:lang="en">cabin</rdfs:label>
