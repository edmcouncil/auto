	<owl:NamedIndividual rdf:about="&auto-vp;Speedometer">
		<rdfs:label xml:lang="en">speedometer</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&auto-vp;SplitServiceBrakeSystem">
		<rdfs:subClassOf rdf:resource="&auto-vp;BrakeSystem"/>
		<rdfs:label>split service brake system</rdfs:label>
		<skos:definition>brake system consisting of two or more subsystems actuated by a single control designed so that a leakage-type failure of a pressure component in a single subsystem (except structural failure of a housing that is common to two or more subsystems) shall not impair the operation of any other subsystem</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;StartingSystem">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
		<rdfs:label>starting system</rdfs:label>
		<skos:definition>vehicle system used in conjunction with the key to activate the engine or motor</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;SteeringColumn">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;SteeringControlSystem"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">steering column</rdfs:label>
