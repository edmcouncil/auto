		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Tread"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">tread rib</rdfs:label>
		<skos:definition xml:lang="en">tread section running circumferentially around a tire</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;Trunk">
		<rdfs:subClassOf rdf:resource="&auto-vp;VehiclePart"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Body"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">trunk</rdfs:label>
