			<owl:Restriction>
				<owl:onProperty rdf:resource="&auto-mo;isPartOf"/>
				<owl:allValuesFrom rdf:resource="&auto-vp;Drivetrain"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">electric motor</rdfs:label>
		<rdfs:comment xml:lang="en">Signal/Attribute.Drivetrain.ElectricMotor : Electric Motor specific signals.</rdfs:comment>
		<skos:definition xml:lang="en">electro-mechanical machine which converts electrical energy into mechanical energy</skos:definition>
	</owl:Class>
	
