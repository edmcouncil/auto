	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ActiveDrivingAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">simultaneous use of Lane Centering Assistance and Adaptive Cruise Control features</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ActiveParkingAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that assists with steering and potentially other functions during parking maneuvers</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;AdaptiveCruiseControlSystem"/>
		<owl:annotatedTarget xml:lang="en">cruise control that also assists with acceleration and/or braking to maintain a driver-selected gap to the vehicle in front</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Airbag"/>
		<owl:annotatedTarget xml:lang="en">inflatable occupant restraint device that is designed to deploy in a side impact crash to help mitigate occupant injury and/or ejection</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>49 C.F.R § 563.5</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;AutomaticEmergencyBrakingSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects potential collisions with a vehicle ahead, provides forward collision warning, and automatically brakes to avoid a collision or lessen the severity of impact</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;AutomaticEmergencySteeringSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects potential collisions with a vehicle ahead and automatically steers to avoid or lessen the severity of impact</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;AutomaticHighBeamsSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that switches between high and low beam headlamps automatically based on lighting and traffic</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;BackupCameraSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that displays the area behind the vehicle when in reverse gear</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;BatteryManagement"/>
		<owl:annotatedTarget xml:lang="en">device or solution that manages battery performance, including controlling the charging and discharging of batteries, electrochemical cells, and fuel cells</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.lawinsider.com/contracts/gVxbRvWuXJi#battery-management</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;BlindSpotWarningSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects vehicles in the blind spot while driving and notifies the driver to their presence</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Catalyst"/>
		<owl:annotatedTarget xml:lang="en">canister in the exhaust system, usually situated before the muffler, containing a substance that reacts chemically with the exhaust in order to reduce harmful emissions</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.edmunds.com/glossary/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;DirectDriverMonitoringSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects the driver’s eye and/or head movement to estimate where the driver is looking</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;DriverReengagementSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that generates a series of escalating warnings and interventions attempting to engage an unresponsive driver</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ElectricMotor"/>
		<owl:annotatedTarget xml:lang="en">electro-mechanical machine which converts electrical energy into mechanical energy</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://circuitglobe.com/electric-motor.html</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ForwardCollisionWarningSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects a potential collision with a vehicle ahead and alerts the driver</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;HVAC"/>
		<owl:annotatedTarget xml:lang="en">system used for moving air between indoor and outdoor areas along with heating and cooling</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.petro.com/resource-center/what-is-hvac</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;HeadUpDisplaySystem"/>
		<owl:annotatedTarget xml:lang="en">feature that projects information relevant to driving into the driver’s forward line of sight</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Hood"/>
		<owl:annotatedTarget xml:lang="en">exterior movable body panel forward of the windshield that is used to cover an engine, luggage, storage, or battery compartment</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>49 C.F.R. § 571.113</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;IndirectDriverMonitoringSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that observes vehicle states, motions and/or driver performance indicators to estimate driver distraction, inattention, or misuse</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Infotainment"/>
		<owl:annotatedTarget xml:lang="en">vehicle system that combines entertainment and information delivery to drivers and passengers</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.techopedia.com/definition/27778/in-vehicle-infotainment-ivi</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;InternalCombustionEngine"/>
		<owl:annotatedTarget xml:lang="en">heat engine in which the combustion that generates the heat takes place inside the engine proper instead of in a furnace</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/internal%20combustion%20engine</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;LaneCenteringAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that provides steering support to assist the driver in continuously maintaining the vehicle at or near the center of the lane</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;LaneDepartureWarningSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that monitors vehicle’s position within the driving lane and alerts driver as the vehicle approaches or crosses lane markers</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;LaneKeepingAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that provides steering support to assist the driver in keeping the vehicle in the lane</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Lidar"/>
		<owl:annotatedTarget xml:lang="en">device or system that emits pulsed laser light and processes their reflections for display and is used especially for detecting and locating objects</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/lidar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Lumbar"/>
		<owl:annotatedTarget xml:lang="en">mechanical or pneumatic device that matches a vehicle&apos;s seat back to the contours of the driver or passenger</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.edmunds.com/glossary/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Mirrors"/>
		<owl:annotatedTarget xml:lang="en">polished or smooth surfaces (as of glass) that form images by reflection</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/mirror</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;NightVisionSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that improves forward visibility at night by projecting enhanced images on instrument cluster or head-up display</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ParkingBrake"/>
		<owl:annotatedTarget xml:lang="en">mechanism designed to prevent the movement of a stationary motor vehicle</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>49 C.F.R. § 571.3</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ParkingCollisionWarningSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects objects close to the vehicle during parking maneuvers and notifies the driver</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Radar"/>
		<owl:annotatedTarget xml:lang="en">device or system that emits radio waves and processes their reflections for display and is used especially for detecting and locating objects</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/radar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;RearCrossTrafficWarningSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects vehicles approaching from the side at the rear of the vehicle while in reverse gear and alerts the driver</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;RemoteParkingAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that without the driver being physically present inside the vehicle, provides steering, braking, accelerating and/or gear selection while moving a vehicle into or out of a parking space</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;ReverseAutomaticEmergencyBrakingSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that detects potential collisions while in reverse gear and automatically brakes to avoid or lessen the severity of impact</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;Roof"/>
		<owl:annotatedTarget>portion of an automobile that sits above the passenger compartment, protecting the vehicle occupants from sun, wind and other external elements</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://en.wikipedia.org/wiki/Automobile_roof</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;SurroundViewCameraSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that displays the immediate surroundings of some or all sides of the vehicle while stopped or during low speed maneuvers</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
	<owl:Axiom>
		<owl:annotatedProperty rdf:resource="&skos;definition"/>
		<owl:annotatedSource rdf:resource="&auto-vp;TrailerAssistanceSystem"/>
		<owl:annotatedTarget xml:lang="en">feature that assists the driver with visual guidance while backing towards a trailer or during backing maneuvers with a trailer attached</owl:annotatedTarget>
		<fibo-fnd-utl-av:adaptedFrom>https://newsroom.aaa.com/wp-content/uploads/2022/07/Clearing-the-Confusion-One-Pager-New-Version-7-25-22.pdf</fibo-fnd-utl-av:adaptedFrom>
	</owl:Axiom>
