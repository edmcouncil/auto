	</owl:Class>
	
	<owl:Class rdf:about="&auto-vp;Tiedown">
		<rdfs:subClassOf rdf:resource="&auto-vp;CargoEquipment"/>
		<rdfs:label xml:lang="en">tiedown</rdfs:label>
