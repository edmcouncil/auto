	<owl:Class rdf:about="&auto-vp;SurgeBrakeSystem">
		<rdfs:subClassOf rdf:resource="&auto-vp;BrakeSystem"/>
		<rdfs:label xml:lang="en">surge brake system</rdfs:label>
