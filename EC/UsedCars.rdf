    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/airbagDeployment -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/airbagDeployment">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/DamageOrAccident"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This property indicates whether at least one of the airbags of the vehicle has been deployed in the respective accident (when attached to uco:DamageOrAccident) or ever (when directly applied to a vso:Vehicle).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">airbag deployment (0..1)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/countryOfOrigin -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/countryOfOrigin">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The country or geo-political region in which the vehicle was initially sold officially, indicated as the two-character version of ISO 3166-1 (ISO 3166-1 alpha-2) for regions or ISO 3166-2, which breaks down the countries from ISO 3166-1 into administrative subdivisions.</rdfs:comment>
