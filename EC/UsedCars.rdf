    <!-- http://schema.org/image -->

    <owl:ObjectProperty rdf:about="http://schema.org/image">
        <owl:equivalentProperty rdf:resource="http://xmlns.com/foaf/0.1/depiction"/>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/depiction -->

    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/depiction"/>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/carriedOutBy -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/carriedOutBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/Check"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/DamageOrAccident"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/ModificationOrMaintenance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/BusinessEntity"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/BusinessEntityType"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The business or type of business that carried out the check, repair, maintenance, or modification</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">carried out by (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/contains -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/contains">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:comment xml:lang="en">A naive part-whole relation that allows providing more granular uco:OwnershipInfo data for a part of the duration of the ownership</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">contains (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/currentLocation -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/currentLocation">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Place"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/PostalAddress"/>
        <rdfs:comment xml:lang="en">The current location of the vehicle, i.e. where the car could be tested. For indicating alternative locations to which the seller is willing to bring the car, use gr:availableAtOrFrom attached to the gr:Offering, since those are aspects of the offer, not of the vehicle.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">currentLocation (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/currentOwner -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/currentOwner">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/owner"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/BusinessEntity"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Organization"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Person"/>
        <rdfs:comment xml:lang="en">This property points to the current owner of the vehicle.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">currentOwner (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/driver -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/driver">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Person"/>
        <rdfs:comment xml:lang="en">This property points to the driver or drivers that were regularly driving the vehicle during the respective period of time (when attached to uco:OwnershipInfo) or in total (when attached directly to a vso:Vehicle).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">driver (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/eventInformation -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/eventInformation">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Event"/>
        <rdfs:comment xml:lang="en">This property points to events related to the vehicle, like accidents and damages or major modifications or maintenance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">eventInformation (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/feature -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/feature">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/PropertyValue"/>
        <rdfs:comment xml:lang="en">This property points to vehicle features in a semi-structured property-value form.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">feature (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/hasOwnershipInfo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/hasOwnershipInfo">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:comment xml:lang="en">This property points to information about the current and previous owners of the vehicle.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">hasOwnershipInfo (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/inspected -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/inspected">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/Check"/>
        <rdfs:comment xml:lang="en">This property points to inspections and certifications that the vehicle has undergone.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">inspected (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/listPrice -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/listPrice">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/PriceSpecification"/>
        <rdfs:comment xml:lang="en">The original list price of the new car at the time of production</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">listPrice (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mainLocation -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mainLocation">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Place"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/PostalAddress"/>
        <rdfs:comment xml:lang="en">The main location(s) of the vehicle during the period of ownership</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">main location (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileage -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileage">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/Check"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/DamageOrAccident"/>
                    <rdf:Description rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/ModificationOrMaintenance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/QuantitativeValue"/>
        <rdfs:comment xml:lang="en">The mileage of the vehicle at the event, e.g. at the accident, maintenance, check, or modification. 

Typical unit code(s): KMT for kilometers, SMI for statute miles</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">mileage (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileageEnd -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileageEnd">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/QuantitativeValue"/>
        <rdfs:comment xml:lang="en">The mileage of the vehicle at the end of the period of ownership. 

Typical unit code(s): KMT for kilometers, SMI for statute miles</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">mileage end (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileageInitial -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/mileageInitial">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/QuantitativeValue"/>
        <rdfs:comment xml:lang="en">The mileage of the vehicle at the beginning of the period of ownership. 

Typical unit code(s): KMT for kilometers, SMI for statute miles</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">mileage initial (0..1)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/owner -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/owner">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/BusinessEntity"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Organization"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/Person"/>
        <rdfs:comment xml:lang="en">This property points to the owner of the vehicle for the respective period of time, as indicated by the uco:OwnershipInfo node.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">owner (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/parking -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/parking">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/ParkingType"/>
        <rdfs:comment xml:lang="en">The main type(s) of parking that have been used for the vehicle during the period of ownership (if attached to uco:OwnershipInfo) or in general (if attached to the vso:Vehicle directly)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">parking (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/pets -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/pets">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/Species"/>
        <rdfs:comment xml:lang="en">The type(s) of animals that have been transported in the vehicle during the period of ownership (if attached to uco:OwnershipInfo) or in general (if attached to the vso:Vehicle directly). 
	
If possible, use DBPedia URIs, e.g. http://dbpedia.org/resource/Dog.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">pets (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/usage -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/usage">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/OwnershipInfo"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/UsageType"/>
        <rdfs:comment xml:lang="en">The type(s) of usage during the period of ownership (if attached to uco:OwnershipInfo) or in general (if attached to the vso:Vehicle directly)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">usage (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/valuation -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/EC/UsedCars/valuation">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/EC/AutoSchemaOrg/Vehicle"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/PriceSpecification"/>
        <rdfs:comment xml:lang="en">The market price assessment gained by the respective valuation. Use gr:name to indicate the name of the valuation service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/uco/ns#"/>
        <rdfs:label xml:lang="en">valuation (0..*)</rdfs:label>
    </owl:ObjectProperty>
    


